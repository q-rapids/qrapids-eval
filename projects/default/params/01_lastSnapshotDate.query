{
	"query": {
    	"bool": {
			"must" : [
        		{ "term" : { "bcKey" : "{{bcKey}}" } },
          		{ "range" : { "snapshotDate" : { "lte" : "{{evaluationDate}}", "format": "yyyy-MM-dd" } } }
      		]
		}
  	},
	"size": 1,
	"sort": [
    	{
			"snapshotDate": {
				"order": "desc"
      		}
    	}
	]
}
